student_scores = {
  "Harry": 81,
  "Ron": 78,
  "Hermione": 99, 
  "Draco": 74,
  "Neville": 62,
}
# ðŸš¨ Don't change the code above ðŸ‘†

#TODO-1: Create an empty dictionary called student_grades.
student_grades = {}

#TODO-2: Write your code below to add the grades to student_grades.ðŸ‘‡
for grade in student_scores:
    # print(grade)
    score = student_scores[grade]
    student_grades
    if student_scores[grade] >= 91:
        student_grades[grade] = "Outstanding"
        # print("'Outstanding'")
    elif student_scores[grade] >= 81 <= 90:
        student_grades[grade] = "Exceeds Expectations"
        # print("'Exceeds expectations'")
    elif student_scores[grade] >= 71 <= 80:
        student_grades[grade] = "Acceptable"
        # print("'Acceptable'")
    else:
        student_grades[grade] = "Fail"
        # print("'Fail'")
# ðŸš¨ Don't change the code below ðŸ‘‡
print(student_grades)
